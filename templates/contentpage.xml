<?xml version="1.0" encoding="UTF-8"?>
<change:templates xmlns:change="http://www.rbs.fr/change/1.0/schema" templateType="tableCells">
	<change:template id="contentpage">
		<div class="container_12" id="top">
			<div id="header">
				<div class="grid_12" id="header-top">
					<div class="grid_7 alpha">
						<change:templateblock type="modules_website_ConfigurableMenu" editname="header-top-1" __mode="tag" __tag="contextual_website_website_menu-header" __displayMode="inline" __depth="1" />
					</div>
					<div class="grid_2"><change:templateblock editname="header-top-2" /></div>
					<div class="grid_2"><change:templateblock type="modules_users_showinfo" editname="header-top-3" /></div>
					<div class="grid_1 omega">
						<change:templateblock type="modules_website_switchlanguage" editname="header-top-4" __viewall="false" __showflag="" __displayMode="Scroll"  />
					</div>
					<div class="clear"></div>
				</div>
				<div class="grid_12" id="header-middle">
					<change:templateblock type="modules_website_xhtmltemplate" editname="header-middle-1" __template="Logo" />
				</div>
				<div class="clear"></div>
				<div class="grid_12" id="header-bottom">
					<change:templateblock type="modules_website_ConfigurableMenu" editname="header-bottom-1" __mode="tag" __tag="contextual_website_website_menu-main" __displayMode="scroll" __depth="2" />
				</div>
				<div class="clear"></div>
			</div>
			<div id="content">
				<div id="content-top">
					<div class="grid_9">
						<change:templateblock type="modules_website_thread" editname="content-top-1" />
					</div>
					<div class="grid_3">
						<change:templateblock type="modules_solrsearch_form" editname="content-top-2" />
					</div>
					<div class="clear"></div>
				</div>
				<div class="clear"></div>
				<div class="grid_12" id="content-middle">
					<div class="grid_3 alpha" id="sidebar-column">
						<change:templateblock type="modules_website_ConfigurableMenu" editname="content-middle-1" __mode="contextual" __displayMode="contextual" __deployAll="false" __showTitle="true" __depth="2" __startLevel="1" />
						<change:content id="sidebar-block"/>
					</div>
					<div class="grid_9 omega" id="content-column">
						<change:content id="content-block"/>
					</div>
					<div class="clear"></div>
				</div>
				<div class="clear"></div>
			</div>
			<div id="footer">
				<div class="grid_12" id="footer-middle">
					<div id="footer-middle-blocks">		
						<div class="grid_3 alpha">
							<change:templateblock editname="footer-middle-1" />
						</div>
						<div class="grid_3">
							<change:templateblock editname="footer-middle-2" />
						</div>
						<div class="grid_3">
							<change:templateblock editname="footer-middle-3" />
						</div>
						<div class="grid_3 omega">
							<change:templateblock editname="footer-middle-4" />
						</div>
						<div class="clear"></div>
					</div>
				</div>
				<div class="clear"></div>
				<div class="grid_12" id="footer-bottom">
					<div class="grid_6 alpha">
						<change:templateblock type="modules_website_ConfigurableMenu" editname="footer-bottom-1" __mode="tag" __tag="contextual_website_website_menu-footer" __displayMode="inline" __depth="1" />
					</div>
					<div class="grid_6 omega">
						<change:templateblock type="modules_website_xhtmltemplate" editname="footer-bottom-2" __template="Copyrights" />
					</div>
				</div>
				<div class="clear"></div>
			</div>
		</div>
	</change:template>
	<change:template content-type="xul">
		<hbox id="top">
			<vbox id="sidebar-column">
				<change:content id="sidebar-block"/>
			</vbox>
			<vbox id="content-column">
				<change:content id="content-block" />
			</vbox>
		</hbox>
	</change:template>
</change:templates>